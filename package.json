{
  "name": "bundle-builder",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "> 0.2% and not dead"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write '**/*.{css,html,js,md,ts,tsx}' --log-level error",
    "lint:js": "next lint",
    "lint:styles": "stylelint --config stylelint.config.js 'src/**/*.css'",
    "lint:ts:watch": "yarn lint:ts --watch",
    "lint:ts": "tsc --project tsconfig.json",
    "lint": "yarn lint:styles && yarn lint:js && yarn lint:ts",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.4",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.12",
    "@svgr/webpack": "^8.0.1",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "classnames": "^2.3.2",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-mixins": "^9.0.4",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.1.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.0.0",
    "react-loading-skeleton": "^3.3.1",
    "stylelint": "^15.10.2",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "svgo": "^3.0.2",
    "uuid": "^9.0.0"
  }
}
